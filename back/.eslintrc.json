{
    "env": {
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended"
      ],
      "rules": {
        "no-underscore-dangle": "off",
        "import/no-named-as-default": "off",
        "no-plusplus": "off",
        "semi": [
          "error",
          "never"
        ]
    }
}